package practise;
import java.util.Scanner;


public class PerfectSquareCheck {
	//perfect square=1 4 9 16 25 36.......(1*1,2*2,3*3....)
	public static void main(String[] args) {
		Scanner scan= new Scanner(System.in);
	    System.out.println("Enter the num");
	    int num=scan.nextInt();
	    check(num);	    

}
	public static void check(int num){
		
		for(int i=1;i*i<=num;i++) { // here i*i<=num is bcoz the perfect square of the num is gradually increasing 
			//if the given num value exceeds then it would be not a perfect square
			
				System.out.println( i*i);
			
			
		}
		
		
		
	}
}

